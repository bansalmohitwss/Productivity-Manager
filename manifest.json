{
    "manifest_version":2,
    "name":"Productivity-Manager",
    "version": "1.0",
    "description": "",
    
    "browser_action":{
        "default_popup":"popup.html",
        "default_title":"Productivity-Manager"
    },
    "background": {
        "scripts": [
          "objectparser.js",
          "background.js"]
    },
    "permissions": [
        "tabs",
        "storage",
        "http://*/*",
        "https://*/*",
        "alarms"
    ]
      
}